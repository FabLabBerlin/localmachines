{
  "name": "easylab-machine-client",
  "version": "1.0.0",
  "description": "No comments.",
  "main": "index.js",
  "repository": "https://github.com/FabLabBerlin/localmachines",
  "scripts": {
    "pretest": "eslint src",
    "test": "jest",
    "dev": "webpack --watch --progress --colors",
    "prod": "NODE_ENV=production webpack -p --progress --colors"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.1.4",
    "bootstrap-less": "^3.3.8",
    "css-loader": "^0.15.3",
    "eslint": "^0.24.1",
    "fbjs": "^0.8.3",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "jest-cli": "^13.2.2",
    "jquery": "^2.1.4",
    "js-cookie": "^2.1.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "lodash": "^3.10.0",
    "moment": "^2.10.6",
    "moment-duration-format": "^1.3.0",
    "node-libs-browser": "^0.5.2",
    "nuclear-js": "^1.1.1",
    "react": "^15.2.0",
    "react-router": "^2.5.2",
    "style-loader": "^0.12.3",
    "toastr": "^2.1.0",
    "url-loader": "^0.5.6",
    "vex-js": "^2.3.2",
    "webpack": "^1.9.10"
  },
  "jest": {
    "rootDir": ".",
    "testDirectoryName": "__test__",
    "scriptPreprocessor": "<rootDir>/preprocessor",
    "unmockedModulePathPatterns": [
      "fbjs",
      "react",
      "<rootDir>/actionTypes.js",
      "<rootDir>/modules/Machines/actionTypes.js"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true
    },
    "globals": {
      "console": true,
      "document": true,
      "clearInterval": true,
      "setInterval": true,
      "setTimeout": true,
      "WebSocket": true,
      "window": true,
      "//": "Global for Unit tests:",
      "describe": true,
      "expect": true,
      "it": true,
      "jasmine": true,
      "jest": true,
      "require": true
    },
    "rules": {
      "no-console": 0,
      "no-extra-bind": 0,
      "no-script-url": 0,
      "no-trailing-spaces": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "no-unused-vars": 0,
      "quotes": [
        1,
        "single"
      ],
      "strict": [
        2,
        "never"
      ]
    }
  },
  "dependencies": {
    "react-addons-test-utils": "^15.2.0",
    "react-datepicker": "^0.28.0",
    "react-dom": "^15.2.0"
  }
}
