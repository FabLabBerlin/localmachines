{
  "name": "easylab-machine-client",
  "version": "1.0.0",
  "description": "No comments.",
  "main": "index.js",
  "repository": "https://github.com/FabLabBerlin/localmachines",
  "scripts": {
    "pretest": "eslint src",
    "test": "jest",
    "dev": "webpack --watch --progress --colors",
    "prod": "NODE_ENV=production webpack -p --progress --colors"
  },
  "author": "",
  "license": "Makea",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "bootstrap-less": "^3.3.8",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "fbjs": "^0.8.9",
    "file-loader": "^0.10.0",
    "font-awesome": "^4.7.0",
    "jest-cli": "^18.1.0",
    "jquery": ">=3.5.0"
    "js-cookie": "^2.1.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "moment-duration-format": "^1.3.0",
    "node-libs-browser": "^2.0.0",
    "nuclear-js": "^1.1.1",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-datepicker": "^0.28.0",
    "react-dom": "^15.2.0",
    "react-router": "^2.5.2",
    "style-loader": "^0.13.1",
    "toastr": "^2.1.0",
    "url-loader": "^0.5.7",
    "vex-js": "^2.3.2",
    "webpack": "^2.2.1"
  },
  "jest": {
    "rootDir": ".",
    "testRegex": "/__test__/.*.js$",
    "unmockedModulePathPatterns": [
      "fbjs",
      "react",
      "<rootDir>/actionTypes.js",
      "<rootDir>/modules/Machines/actionTypes.js",
      "<rootDir>/modules/Machines/getters.js",
      "<rootDir>/modules/Machines/index.js"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true
    },
    "globals": {
      "console": true,
      "document": true,
      "clearInterval": true,
      "setInterval": true,
      "setTimeout": true,
      "WebSocket": true,
      "window": true,
      "//": "Global for Unit tests:",
      "describe": true,
      "expect": true,
      "it": true,
      "jasmine": true,
      "jest": true,
      "require": true
    },
    "rules": {
      "no-console": 0,
      "no-extra-bind": 0,
      "no-script-url": 0,
      "no-trailing-spaces": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "no-unused-vars": 0,
      "quotes": [
        1,
        "single"
      ],
      "strict": [
        2,
        "never"
      ]
    }
  },
  "dependencies": {
    "react-datetime": "^2.6.0"
  }
}
